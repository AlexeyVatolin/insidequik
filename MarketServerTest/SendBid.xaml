<Window x:Class="MarketServerTest.SendBid"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarketServerTest"
        mc:Ignorable="d"
        Title="SendBid" Height="300" Width="300">
    <Grid>
        <Button Content="Buy" HorizontalAlignment="Left" Margin="10,205,0,0" VerticalAlignment="Top" Width="75" Click="Buy_Click"/>
        <Button Content="Sell" HorizontalAlignment="Left" Margin="196,205,0,0" VerticalAlignment="Top" Width="75" Click="Sell_Click"/>
        <TextBox x:Name="PriceBox" HorizontalAlignment="Left" Height="23" Margin="10,77,0,0" TextWrapping="Wrap" Text="226,12" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="QuantityBox" HorizontalAlignment="Left" Height="23" Margin="151,77,0,0" TextWrapping="Wrap" Text="5" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="151,59,0,0" TextWrapping="Wrap" Text="Количество" VerticalAlignment="Top"/>
        <TextBox x:Name="TickerBox" HorizontalAlignment="Left" Height="23" Margin="10,52,0,0" TextWrapping="Wrap" Text="SBER" VerticalAlignment="Top" Width="120"/>
        <CheckBox x:Name="MarketPrice" Content="Рыночная цена" HorizontalAlignment="Left" Margin="10,131,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
